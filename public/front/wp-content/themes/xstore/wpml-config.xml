<wpml-config>
  <custom-types>
    <custom-type translate="1">staticblocks</custom-type>
    <custom-type translate="1">etheme_portfolio</custom-type>
    <custom-type translate="1">testimonials</custom-type>
  </custom-types>
  <taxonomies>
    <taxonomy translate="1">portfolio_category</taxonomy>
    <taxonomy translate="1">testimonial-category</taxonomy>
  </taxonomies>
  <admin-texts>
    <key name="et_options">
      <key name="404_text"/>
      <key name="product_bage_banner"/>
      <key name="empty_cart_content"/>
      <key name="custom_tab_title"/>
      <key name="custom_tab"/>
      <key name="promo-link-text"/>
      <key name="pp_content"/>
      <key name="header_custom_block"/>
      <key name="all_departments_text"/>
      <key name="sign_in_text"/>
      <key name="sale_icon_text"/>
      <key name="cart_popup_banner"/>
    </key>
  </admin-texts>
  <shortcodes>
    <shortcode>
      <tag>et_tab</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>banner</tag>
      <attributes>
          <attribute>title</attribute>
          <attribute>subtitle</attribute>
          <attribute type="link" encoding="vc_link">link</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>title</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
          <attribute encoding="allow_html_tags">subtitle</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>icon_box</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>team_member</tag>
    </shortcode>
    <shortcode>
      <tag>twitter</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>instagram</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
          <attribute encoding="allow_html_tags">link</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>etheme_categories</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>etheme_products</tag>
      <attributes>
          <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>et_the_look</tag>
    </shortcode>
    <shortcode>
      <tag>menu</tag>
      <attributes>
        <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>et_menu_list</tag>
      <attributes>
        <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>et_menu_list_item</tag>
      <attributes>
        <attribute encoding="allow_html_tags">title</attribute>
      </attributes>
    </shortcode>
  </shortcodes>
</wpml-config>